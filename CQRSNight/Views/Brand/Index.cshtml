@model List<CQRSNight.CQRSDesignPattern.Results.BrandResults.GetBrandQueryResult>
@{
    Layout = null;
}
<link href="~/lib/bootstrap/dist/css/bootstrap.min.css" rel="stylesheet" />
<div class="container">
    <br />
    <div class="alert alert-primary">Kategori Listesi</div>
    <table class="table table-bordered">
        <tr>
            <th>#</th>
            <th>Marka Adı</th>
            <th>Sil</th>
            <th>Güncelle</th>
        </tr>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.Id</td>
                <td>@item.Name</td>
                <td> <a href="/Brand/RemoveBrand/@item.Id" class="btn btn-danger">Sil</a> </td>
                <td> <a href="/Brand/UpdateBrand/@item.Id" class="btn btn-info">Güncelle</a> </td>
            </tr>
        }
    </table>
    <a href="/Brand/CreateBrand/" class="btn btn-info">Yeni Kategori Girişi</a>
</div>
